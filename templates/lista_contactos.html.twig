        {% for contacto in contactos %}
        <ul>
            <li><strong>{{ contacto.nombre}}</strong></li>
            <li><strong>Tel√©fono: </strong>{{ contacto.telefono}}</li>
            <li><strong>Correo: </strong>{{ contacto.email}}</li>
